{
  "root": true,
  "env": {
    "browser": true
  },
  "extends": ["airbnb", "plugin:vue/recommended", "prettier"],
  "globals": {
    "Vue": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "semi": "off",
    "vue/no-v-html": "off"
  }
}
